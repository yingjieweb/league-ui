{
  "name": "@yingjieweb/league-ui",
  "version": "0.2.6",
  "description": "A Vue.js component library for League of Legends",
  "author": "yingjieweb",
  "private": false,
  "main": "dist/league-ui.umd.min.js",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/yingjieweb"
  },
  "repository": "https://github.com/yingjieweb/league-ui",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "test:unit-watch": "karma start",
    "test": "karma start --single-run",
    "test:unit:vue-cli": "vue-cli-service test:unit",
    "test:unit:convergence": "cross-env NODE_ENV=test karma start --single-run",
    "build": "vue-cli-service build",
    "build:ui": "vue-cli-service build --target lib --name league-ui ./src/entry.js"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-unit-mocha": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/test-utils": "^1.0.3",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.2",
    "karma-mocha": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^4.0.2",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "vue-template-compiler": "^2.6.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
